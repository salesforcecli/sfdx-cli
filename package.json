{
  "name": "sfdx-cli",
  "description": "Salesforce CLI",
  "version": "7.132.0",
  "author": "Salesforce",
  "license": "BSD-3-Clause",
  "bugs": "https://github.com/forcedotcom/cli/issues",
  "homepage": "https://github.com/salesforcecli/sfdx-cli/",
  "repository": "salesforcecli/sfdx-cli",
  "bin": {
    "sfdx": "bin/run"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "bin",
    "oclif.manifest.json",
    "dist/**/*.js",
    "scripts/include-sf.js",
    "!dist/**/*.test.js"
  ],
  "oclif": {
    "bin": "sfdx",
    "scope": "salesforce",
    "commands": "./dist/commands",
    "dirname": "sfdx",
    "hooks": {
      "plugins:preinstall": [
        "./dist/hooks/pluginsPreinstall.js"
      ],
      "plugins:preinstall:verify:version": [
        "./dist/hooks/verifyPluginVersion.js"
      ],
      "preupdate": [
        "./dist/hooks/preupdate.js"
      ],
      "update": [
        "./dist/hooks/lazyRequire.js",
        "./dist/hooks/postupdate.js"
      ]
    },
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-plugins",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-which",
      "@salesforce/plugin-alias",
      "@salesforce/plugin-apex",
      "@salesforce/plugin-auth",
      "@salesforce/plugin-config",
      "@salesforce/plugin-custom-metadata",
      "@salesforce/plugin-data",
      "@salesforce/plugin-generator",
      "@salesforce/plugin-info",
      "@salesforce/plugin-limits",
      "@salesforce/plugin-org",
      "@salesforce/plugin-source",
      "@salesforce/plugin-schema",
      "@salesforce/plugin-telemetry",
      "@salesforce/plugin-templates",
      "@salesforce/plugin-trust",
      "@salesforce/plugin-user",
      "@salesforce/sfdx-plugin-lwc-test",
      "salesforce-alm"
    ],
    "devPlugins": [
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-release-management"
    ],
    "update": {
      "s3": {
        "bucket": "dfc-data-production",
        "indexVersionLimit": 20,
        "folder": "media/salesforce-cli/sfdx",
        "acl": " ",
        "host": "https://developer.salesforce.com"
      }
    },
    "info": {
      "releasenotes": {
        "distTagUrl": "https://registry.npmjs.org/-/package/sfdx-cli/dist-tags",
        "releaseNotesPath": "https://github.com/forcedotcom/cli/tree/main/releasenotes/sfdx",
        "releaseNotesFilename": "README.md"
      }
    },
    "macos": {
      "identifier": "com.salesforce.cli"
    },
    "windows": {
      "name": "Salesforce CLI",
      "keypath": "/tmp/windows-signing.pfx"
    }
  },
  "pinnedDependencies": [
    "@salesforce/plugin-alias",
    "@salesforce/plugin-apex",
    "@salesforce/plugin-auth",
    "@salesforce/plugin-config",
    "@salesforce/plugin-custom-metadata",
    "@salesforce/plugin-data",
    "@salesforce/plugin-info",
    "@salesforce/plugin-limits",
    "@salesforce/plugin-org",
    "@salesforce/plugin-source",
    "@salesforce/plugin-schema",
    "@salesforce/plugin-telemetry",
    "@salesforce/plugin-templates",
    "@salesforce/plugin-user",
    "@salesforce/sfdx-plugin-lwc-test",
    "salesforce-alm"
  ],
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-commands": "^1.3.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-plugins": "^1.10.1",
    "@oclif/plugin-update": "1.5.0",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@oclif/plugin-which": "^1.0.3",
    "@salesforce/kit": "^1.5.13",
    "@salesforce/lazy-require": "^0.4.0",
    "@salesforce/plugin-alias": "1.2.0",
    "@salesforce/plugin-apex": "0.7.0",
    "@salesforce/plugin-auth": "1.8.0",
    "@salesforce/plugin-config": "1.3.2",
    "@salesforce/plugin-custom-metadata": "1.0.12",
    "@salesforce/plugin-data": "0.6.6",
    "@salesforce/plugin-generator": "^1.1.7",
    "@salesforce/plugin-info": "1.1.4",
    "@salesforce/plugin-limits": "1.3.0",
    "@salesforce/plugin-org": "1.11.0",
    "@salesforce/plugin-schema": "1.1.0",
    "@salesforce/plugin-source": "1.6.2",
    "@salesforce/plugin-telemetry": "1.4.0",
    "@salesforce/plugin-templates": "52.7.0",
    "@salesforce/plugin-trust": "^1.0.8",
    "@salesforce/plugin-user": "1.7.0",
    "@salesforce/require-analytics": "^0.9.16",
    "@salesforce/sfdx-plugin-lwc-test": "0.1.7",
    "@salesforce/ts-types": "^1.5.17",
    "debug": "^4.3.1",
    "salesforce-alm": "53.7.0",
    "salesforce-lightning-cli": "3.0.0",
    "shelljs": "^0.8.4",
    "tslib": "^2.1.0",
    "v8-compile-cache": "^2.2.0"
  },
  "devDependencies": {
    "@oclif/plugin-command-snapshot": "^2.0.0",
    "@salesforce/dev-config": "^2.1.2",
    "@salesforce/dev-scripts": "^1.0.2",
    "@salesforce/plugin-release-management": "^2.8.1",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.3.12",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.6",
    "@types/jsforce": "1.9.37",
    "@types/request": "^2.48.5",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "aws-sdk": "^2.920.0",
    "chai": "^4.3.4",
    "check-yarn-lock": "^0.2.1",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "~5.0.3",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.7",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsdoc": "^35.1.2",
    "eslint-plugin-prettier": "^3.4.0",
    "globby": "^11.0.3",
    "got": "^11.8.2",
    "husky": "^7.0.4",
    "mocha": "^8.4.0",
    "npm": "^7.15.1",
    "nyc": "^15.1.0",
    "oclif": "2.0.1",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "shx": "^0.3.3",
    "sinon": "10.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3"
  },
  "resolutions": {
    "@salesforce/source-deploy-retrieve": "5.8.1",
    "@salesforce/ts-types": "1.5.20",
    "@salesforce/plugin-telemetry": "1.4.0",
    "@salesforce/templates": "52.7.0"
  },
  "scripts": {
    "build": "sf-build",
    "clean": "shx rm -rf dist tmp",
    "clean-all": "sf-clean all",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "oclif-artifacts": "oclif manifest .",
    "pack:deb": "oclif pack:deb",
    "pack:docker:full": "./scripts/docker-publish-full.js",
    "pack:docker:slim": "./scripts/docker-publish-slim.js",
    "pack:macos": "oclif pack:macos --additional-cli sf",
    "pack:tarballs": "oclif pack:tarballs",
    "pack:verify": "sf-release cli:tarballs:verify",
    "pack:win": "oclif pack:win --additional-cli sf",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn test:deprecation-policy ",
    "prepack": "yarn compile && yarn lint && yarn oclif-artifacts",
    "prepare": "sf-install && yarn compile && yarn lint",
    "pretarball": "sf-release cli:tarballs:prepare && ./scripts/include-sf.js",
    "promote-dist-tags": "./scripts/promote-dist-tags",
    "promote-docker": "./scripts/docker-promote.js",
    "promote": "oclif promote",
    "snapshot-generate": "HOME=./ ./bin/run snapshot:generate",
    "test": "nyc mocha --recursive \"test/**/*.test.ts\" --report lcov --report cobertura -t 240000 --require source-map-support/register",
    "snapshot-compare": "HOME=./ ./bin/run snapshot:compare",
    "test-smoke-unix": "sf-release cli:tarballs:smoke --cli sfdx",
    "test:deprecation-policy": "set HOME=./ ./bin/run snapshot:compare",
    "trogdor": "yarn clean-all && yarn && yarn compile && yarn uts",
    "upload:deb": "oclif upload:deb",
    "upload:macos": "oclif upload:macos",
    "upload:tarballs": "oclif upload:tarballs",
    "upload:win": "oclif upload:win",
    "uts": "nyc mocha --require source-map-support/register --recursive \"test/**/*.test.ts\"",
    "watch": "tsc -w -p tsconfig.json"
  }
}
