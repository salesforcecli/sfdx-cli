  "bugs": "https://github.com/forcedotcom/cli/issues",
  "homepage": "https://github.com/salesforcecli/sfdx-cli/",
  "repository": "salesforcecli/sfdx-cli",
  "bin": {
    "sfdx": "bin/run"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "bin",
    "oclif.manifest.json",
    "dist/**/*.js",
    "scripts/include-sf.js",
    "scripts/determine-sf-version.js",
    "scripts/post-install-release-notes.js",
    "!dist/**/*.test.js",
    "/npm-shrinkwrap.json"
  ],
  "oclif": {
    "bin": "sfdx",
    "scope": "salesforce",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "flexibleTaxonomy": true,
    "helpClass": "./dist/help/sfdxHelp.js",
    "dirname": "sfdx",
    "additionalVersionFlags": [
      "-v"
    ],
    "additionalHelpFlags": [
      "-h"
    ],
    "hooks": {
      "command_incomplete": [
        "./dist/hooks/incomplete"
      ],
      "plugins:preinstall": [
        "./dist/hooks/pluginsPreinstall.js"
      ],
      "plugins:preinstall:verify:version": [
        "./dist/hooks/verifyPluginVersion.js"
      ],
      "preupdate": [
        "./dist/hooks/preupdate.js"
      ],
      "update": [
        "./dist/hooks/postupdate.js",
        "./dist/hooks/displayReleaseNotes.js",
        "./dist/hooks/salesforcedx-check.js"
      ]
    },
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands",
      "@oclif/plugin-help",
      "@oclif/plugin-version",
      "@oclif/plugin-not-found",
      "@oclif/plugin-plugins",
      "@oclif/plugin-search",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-which",
      "@salesforce/plugin-apex",
      "@salesforce/plugin-auth",
      "@salesforce/plugin-community",
      "@salesforce/plugin-custom-metadata",
      "@salesforce/plugin-data",
      "@salesforce/plugin-deploy-retrieve",
      "@salesforce/plugin-info",
      "@salesforce/plugin-limits",
      "@salesforce/plugin-org",
      "@salesforce/plugin-packaging",
      "@salesforce/plugin-settings",
      "@salesforce/plugin-signups",
      "@salesforce/plugin-source",
      "@salesforce/plugin-schema",
      "@salesforce/plugin-telemetry",
      "@salesforce/plugin-templates",
      "@salesforce/plugin-trust",
      "@salesforce/plugin-user"
    ],
    "jitPlugins": {
      "@salesforce/plugin-sobject": "0.1.25",
      "@salesforce/plugin-dev": "0.7.10",
      "@salesforce/sfdx-plugin-lwc-test": "1.0.2",
      "@salesforce/sfdx-scanner": "3.13.0"
    },
    "devPlugins": [
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-release-management"
    ],
    "update": {
      "s3": {
        "bucket": "dfc-data-production",
        "indexVersionLimit": 140,
        "folder": "media/salesforce-cli/sfdx",
        "acl": " ",
        "host": "https://developer.salesforce.com"
      }
    },
    "info": {
      "releasenotes": {
        "distTagUrl": "https://registry.npmjs.org/-/package/sfdx-cli/dist-tags",
        "releaseNotesPath": "https://github.com/forcedotcom/cli/tree/main/releasenotes/sfdx",
        "releaseNotesFilename": "README.md"
      }
    },
    "macos": {
      "identifier": "com.salesforce.cli"
    },
    "windows": {
      "name": "Salesforce CLI",
      "keypath": "/tmp/windows-signing.pfx"
    }
  },
  "pinnedDependencies": [
    "@oclif/core",
    "@oclif/plugin-autocomplete",
    "@oclif/plugin-commands",
    "@oclif/plugin-help",
    "@oclif/plugin-not-found",
    "@oclif/plugin-plugins",
    "@oclif/plugin-search",
    "@oclif/plugin-update",
    "@oclif/plugin-version",
    "@oclif/plugin-warn-if-update-available",
    "@oclif/plugin-which",
    "@salesforce/core",
    "@salesforce/kit",
    "@salesforce/plugin-apex",
    "@salesforce/plugin-auth",
    "@salesforce/plugin-community",
    "@salesforce/plugin-custom-metadata",
    "@salesforce/plugin-data",
    "@salesforce/plugin-deploy-retrieve",
    "@salesforce/plugin-info",
    "@salesforce/plugin-limits",
    "@salesforce/plugin-org",
    "@salesforce/plugin-packaging",
    "@salesforce/plugin-schema",
    "@salesforce/plugin-settings",
    "@salesforce/plugin-signups",
    "@salesforce/plugin-source",
    "@salesforce/plugin-telemetry",
    "@salesforce/plugin-templates",
    "@salesforce/plugin-trust",
    "@salesforce/plugin-user",
    "@salesforce/sf-plugins-core",
    "@salesforce/ts-types"
  ],
  "dependencies": {
    "@oclif/core": "2.8.7",
    "@oclif/plugin-autocomplete": "2.3.0",
    "@oclif/plugin-commands": "2.2.15",
    "@oclif/plugin-help": "5.2.9",
    "@oclif/plugin-not-found": "2.3.25",
    "@oclif/plugin-plugins": "3.1.2",
    "@oclif/plugin-search": "0.0.17",
    "@oclif/plugin-update": "3.1.18",
    "@oclif/plugin-version": "1.3.4",
    "@oclif/plugin-warn-if-update-available": "2.0.37",
    "@oclif/plugin-which": "2.2.22",
    "@salesforce/core": "4.3.1",
    "@salesforce/kit": "3.0.3",
    "@salesforce/plugin-apex": "2.3.3",
    "@salesforce/plugin-auth": "2.8.1",
    "@salesforce/plugin-community": "2.3.2",
    "@salesforce/plugin-custom-metadata": "2.1.26",
    "@salesforce/plugin-data": "2.3.24",
    "@salesforce/plugin-deploy-retrieve": "1.13.0",
    "@salesforce/plugin-info": "2.6.20",
    "@salesforce/plugin-limits": "2.3.20",
    "@salesforce/plugin-org": "2.9.12",
    "@salesforce/plugin-packaging": "1.20.0",
    "@salesforce/plugin-schema": "2.3.13",
    "@salesforce/plugin-settings": "1.4.13",
    "@salesforce/plugin-signups": "1.4.22",
    "@salesforce/plugin-source": "2.10.15",
    "@salesforce/plugin-telemetry": "2.2.1",
    "@salesforce/plugin-templates": "55.4.21",
    "@salesforce/plugin-trust": "2.4.20",
    "@salesforce/plugin-user": "2.3.17",
    "@salesforce/sf-plugins-core": "3.1.0",
    "@salesforce/ts-types": "2.0.3",
    "debug": "^4.3.4",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@oclif/plugin-command-snapshot": "^4.0.2",
    "@salesforce/dev-config": "^4.0.1",
