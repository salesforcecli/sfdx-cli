{
  "name": "sfdx-cli",
  "description": "Salesforce CLI",
  "version": "7.92.0",
  "author": "Salesforce",
  "license": "BSD-3-Clause",
  "bugs": "https://github.com/forcedotcom/cli/issues",
  "homepage": "https://github.com/salesforcecli/sfdx-cli/",
  "bin": {
    "sfdx": "bin/run"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "bin",
    "oclif.manifest.json",
    "dist/**/*.js",
    "!dist/**/*.test.js"
  ],
  "oclif": {
    "bin": "sfdx",
    "scope": "salesforce",
    "commands": "./dist/commands",
    "node": "14.15.4",
    "dirname": "sfdx",
    "hooks": {
      "plugins:preinstall": [
        "./dist/hooks/pluginsPreinstall.js"
      ],
      "plugins:preinstall:verify:version": [
        "./dist/hooks/verifyPluginVersion.js"
      ],
      "preupdate": [
        "./dist/hooks/preupdate.js"
      ],
      "update": [
        "./dist/hooks/lazyRequire.js"
      ]
    },
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-plugins",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-which",
      "@salesforce/plugin-generator",
      "@salesforce/plugin-config",
      "@salesforce/plugin-alias",
      "@salesforce/plugin-auth",
      "@salesforce/sfdx-trust",
      "@salesforce/plugin-telemetry",
      "salesforcedx"
    ],
    "devPlugins": [
      "@oclif/plugin-command-snapshot"
    ],
    "update": {
      "s3": {
        "bucket": "dfc-data-production",
        "folder": "media/salesforce-cli/sfdx",
        "acl": " ",
        "host": "https://developer.salesforce.com",
        "templates": {
          "target": {
            "baseDir": "sfdx-cli-v<%- version %>-<%- platform === 'win32' ? 'windows' : platform %>-<%- arch %>",
            "manifest": "media/salesforce-cli/sfdx-cli/channels/<%- channel %>/<%- platform === 'win32' ? 'windows' : platform %>-<%- arch %>",
            "versioned": "media/salesforce-cli/sfdx-cli/channels/<%- channel %>/sfdx-cli-v<%- version %>-<%- platform === 'win32' ? 'windows' : platform %>-<%- arch %>.tar.<%- ext %>"
          }
        }
      }
    },
    "macos": {
      "identifier": "com.salesforce.cli"
    },
    "windows": {
      "name": "Salesforce CLI",
      "keypath": "./tmp/windows-signing.pfx"
    }
  },
  "pinnedDependencies": [
    "@salesforce/plugin-config",
    "@salesforce/plugin-alias",
    "@salesforce/plugin-auth",
    "@salesforce/plugin-telemetry",
    "salesforcedx"
  ],
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-commands": "^1.3.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-plugins": "^1.9.5",
    "@oclif/plugin-update": "1.4.0-2",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@oclif/plugin-which": "^1.0.3",
    "@salesforce/kit": "^1.4.0",
    "@salesforce/lazy-require": "^0.4.0",
    "@salesforce/plugin-alias": "1.1.8",
    "@salesforce/plugin-auth": "1.4.10",
    "@salesforce/plugin-config": "1.2.6",
    "@salesforce/plugin-generator": "^1.1.0",
    "@salesforce/plugin-telemetry": "1.1.1",
    "@salesforce/require-analytics": "^0.7.0",
    "@salesforce/sfdx-trust": "^3.6.0",
    "@salesforce/ts-types": "^1.5.0",
    "debug": "^4.3.1",
    "salesforcedx": "51.4.0",
    "shx": "^0.3.3",
    "tslib": "^2.1.0",
    "v8-compile-cache": "^2.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-command-snapshot": "^2.0.0",
    "@salesforce/dev-config": "^2.1.0",
    "@salesforce/dev-scripts": "^0.9.1",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.3.0",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.6",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "aws-sdk": "^2.836.0",
    "chai": "^4.2.0",
    "check-yarn-lock": "^0.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "~5.0.3",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-salesforce": "^0.1.0",
    "eslint-config-salesforce-license": "^0.1.0",
    "eslint-config-salesforce-typescript": "^0.2.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^27.0.3",
    "eslint-plugin-prettier": "^3.1.3",
    "globby": "^11.0.2",
    "husky": "^4.2.5",
    "mocha": "^8.3.0",
    "npm": "^7.5.2",
    "nyc": "^15.1.0",
    "oclif": "^1.16.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "shelljs": "^0.8.4",
    "sinon": "^9.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "XXpostpack:tarballs": "./scripts/verify-tarballs",
    "XXprepack:tarballs": "./scripts/clean-for-tarballs",
    "build": "sf-build",
    "clean": "shx rm -rf dist tmp",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "eslint \"src/**/*.ts\"",
    "oclif-artifacts": "oclif manifest",
    "pack:deb": "oclif pack:deb",
    "pack:macos": "oclif pack:macos",
    "pack:tarballs": "oclif pack:tarballs",
    "pack:win": "oclif pack:win",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "yarn test:deprecation-policy ",
    "prepack": "yarn clean && yarn compile && yarn lint && yarn oclif-artifacts",
    "prepare": "yarn compile && yarn lint",
    "promote": "oclif promote",
    "snapshot-generate": "HOME=./ ./bin/run snapshot:generate",
    "test": "nyc --report lcov --report cobertura mocha -t 240000 --require source-map-support/register --recursive \"dist/**/*.test.js\"",
    "test-smoke-unix": "./tmp/sfdx/bin/sfdx version && ./tmp/sfdx/bin/sfdx help && ./tmp/sfdx/bin/sfdx plugins --core",
    "test:deprecation-policy": "set HOME=./ ./bin/run snapshot:compare",
    "trogdor": "yarn clean-all && yarn && yarn compile && yarn uts",
    "upload:deb": "oclif upload:deb",
    "upload:macos": "oclif upload:macos",
    "upload:tarballs": "oclif upload:tarballs",
    "upload:win": "oclif upload:win",
    "uts": "nyc mocha --require source-map-support/register --recursive \"dist/**/*.test.js\"",
    "watch": "tsc -w -p tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sf-husky-pre-commit",
      "commit-msg": "sf-husky-commit-msg"
    }
  }
}
